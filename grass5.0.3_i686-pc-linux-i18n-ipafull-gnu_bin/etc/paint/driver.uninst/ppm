:


HRES=100.0
VRES=100.0
NCHARS=1

TEXTSCALE=1
TEXTFUDGE=0
TEXTSPACE=1
BLOCKSIZE=1
BLOCKSPACE=1
NBLOCKS=1

MAPLP=${MAPLP-$HOME/paint.ppm}
WIDTH=800
HEIGHT=800
cp /dev/null $MAPLP
export HRES VRES NCHARS MAPLP WIDTH HEIGHT
export TEXTSCALE TEXTSPACE TEXTFUDGE BLOCKSIZE BLOCKSPACE NBLOCKS

exec ${PAINT_DRIVER?}
