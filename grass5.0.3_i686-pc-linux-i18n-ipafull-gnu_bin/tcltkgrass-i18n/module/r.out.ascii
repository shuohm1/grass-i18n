interface_build {
    {r.out.ascii} 0
    {Converts a raster map layer into an ASCII text file.}
    {entry input {Input raster map:} 0 raster}
    {entry output {Output ASCII file:} 0 file}
    {entry dp {Minimum number of digits (per cell) to be printed:} 0 ""}
    {checkbox -h {No header.} "" -h}
    {checkbox -s {Write SURFER (Golden Software) GRID instead of GRASS GRID} "" -s}
    {checkbox -i {Output integer category values, not cell values.} "" -i}
}
